{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "**",
      "function": "app"
    }],
    "redirects": [{
      "source": "/vaccine-tracker",
      "destination": "https://cmorton.shinyapps.io/VaccinationDash/",
      "type": 301
    }]
  }
}
